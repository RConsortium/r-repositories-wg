foreach f (tests tests-devel tests-clang tests-noLD tests-gcc-SAN tests-clang-SAN tests-ATLAS tests-MKL tests-OpenBLAS  tests-clang-trunk tests-gcc12)
rm -f ~/R/packages/$f/*/src/symbols.rds
rm -f ~/R/packages/$f/*/src/*.so
rm -f ~/R/packages/$f/*/src/*.mod
find ~/R/packages/$f -name \*.o -delete
end
